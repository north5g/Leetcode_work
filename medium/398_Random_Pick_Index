class Solution:
    def __init__(self, nums: List[int]):
        self.hash = {}
        for i, number in enumerate(nums):
            if number in self.hash:
                self.hash[number].append(i)
            else:
                self.hash[number] = [i]

    def pick(self, target: int) -> int:
        return self.hash[target][randint(0, len(self.hash[target]) - 1)]