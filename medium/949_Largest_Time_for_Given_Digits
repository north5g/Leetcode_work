class Solution:
    def largestTimeFromDigits(self, arr: List[int]) -> str:
        # (2 - 0) (3 - 0) : (5 - 0) (9 - 0)
        # no numbers less than 2
        # more than one number greater than 5
        answer = ""
        for P in permutations(arr):
            if P[0]*10+P[1] <= 23 and P[2] <=5:
                answer = max(answer, str(P[0])+str(P[1])+":"+str(P[2])+str(P[3]))
        return answer