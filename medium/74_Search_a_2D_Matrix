class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        found = len(matrix)
        for i in range(len(matrix)):
            if matrix[i][0] > target:
                found = i
                break
        
        found -= 1
        if found == -1:
            return False
        
        for j in range(len(matrix[0])):
            if matrix[found][j] == target:
                return True
        return False
