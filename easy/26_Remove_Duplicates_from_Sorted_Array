class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        index = 0
        j = 1
        while j < len(nums):
            if nums[index] != nums[j]:
                index += 1
                nums[index] = nums[j]
            j += 1
        return index + 1